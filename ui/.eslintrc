{
  "parser": "babel-eslint",
  "rules": {
    "camelcase": "off",
    "no-unused-vars": "warn",
    "no-undef": "off",
    "no-constant-condition": "warn",
    "react/prop-types": "warn",
    "react/no-unescaped-entities": "off"
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
